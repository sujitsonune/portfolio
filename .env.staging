# Staging Environment Configuration
NODE_ENV=production
NEXT_PUBLIC_APP_ENV=staging
NEXT_PUBLIC_APP_URL=https://portfolio-sujit-staging.vercel.app

# Firebase - Staging Project
NEXT_PUBLIC_FIREBASE_PROJECT_ID=portfolio-sujit-staging

# Staging-specific features
NEXT_PUBLIC_SHOW_PERFORMANCE_MONITOR=false
ANALYZE_BUNDLE=true
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=true

# Security settings
NEXTAUTH_URL=https://portfolio-sujit-staging.vercel.app

# Staging logging
LOG_LEVEL=info
ENABLE_DEV_LOGGING=false